	"private": true,
	"engines": {
		"node": "9.3.0",
		"npm": "5.5.1"
	},
	"jest": {
		"setupTestFrameworkScriptFile": "./tests/setup.js"
	},
	"scripts": {
		"start": "node index.js",
		"server": "nodemon index.js",
